<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Content/JQueryUI/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/JQueryUI/jquery-ui.theme.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/ContextMenu/contextMenu.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/highcharts")
    <script src="~/Scripts/jqSimpleConnect-1.0.js"></script>
    <script src="~/Scripts/contextMenu.js"></script>
    <script src="~/Scripts/lib.js"></script>


    @RenderSection("scripts", required: false)
</head>
<body>
    @RenderBody()
</body>
</html>
